@model testMVC.Models.ApplicationUser
@{
    ViewData["Title"] = Model.Name;
    //Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<ul>
    <li>Email: @Model.Email</li>
    <li>Phone number: @Model.Phone</li>
    <li>Card number: @Model.CreditCard</li>
    <li>Birthday: @Model.BirthDate.Value.ToShortDateString()</li>
</ul>